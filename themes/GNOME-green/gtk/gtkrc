}
style "clearlooks-menu"
{
  bg[NORMAL] = "#f9f7f3"
  bg[PRELIGHT]      = "#4b6983"
  fg[PRELIGHT]    = "#ffffff" 
}

style "clearlooks-note"
{
  #bg[NORMAL] = "#eae4df"
  #bg[INSENSITIVE] = "#eae4df"
  #base[INSENSITIVE] = "#eae4df"
}

