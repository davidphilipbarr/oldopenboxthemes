{
  #bg[NORMAL] = "#f3f0ed"
  #bg[INSENSITIVE] = "#f3f0ed"
  #base[INSENSITIVE] = "#eae4df"
  #bg[PRELIGHT]      = "#f8f5f2"
}


class "GtkWidget" style "clearlooks-default"
class "GtkMenu" style "clearlooks-menu"
