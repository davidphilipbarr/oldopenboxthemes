 	bg[NORMAL]	  = "#D6D6D6"
	bg[PRELIGHT]	  = "#dae1e4"
}

style "check" {
	base[SELECTED]    = "#D1E1E8"
}  

style "note" {

	bg[NORMAL] =  "#e9e9e9"
}  

 

style "status" {
 	bg[PRELIGHT]	  = "#D1E1E8"
	bg[SELECTED]	  = "#D1E1E8"
	base[ACTIVE]	  = "#D1E1E8"
	base[SELECTED]=  "#D1E1E8"
 	base[PRELIGHT]	  = "#D1E1E8"
}

