style "default"
{


	fg[NORMAL]        = "#111111"
	fg[PRELIGHT]      = "#111111"
	fg[SELECTED]      = "#111111"
	fg[ACTIVE]        = "#111111"
	fg[INSENSITIVE]   = "#777777"

	bg[NORMAL]        = "#e0e0e0"
        bg[PRELIGHT]	  = "#E9E9E9"
	bg[SELECTED]	  = "#D1E1E8"
	bg[INSENSITIVE]   = "#d0d0d0"
	bg[ACTIVE]        = "#D1D1D1"

	base[NORMAL]      = "#ffffff"
	base[PRELIGHT]    = "#D1E1E8"
	base[ACTIVE]      = "#d0d0d0"
	base[SELECTED]    = "#d0d0d0"
	base[INSENSITIVE] = "#d0d0d0"

	text[NORMAL]      = "#111111"
	text[PRELIGHT]    = "#111111"
	text[ACTIVE]      = "#111111"
	text[SELECTED]    = "#111111"
	text[INSENSITIVE] = "#888888"

#engine "mist" {}

}

